apiVersion: v1
kind: Service
metadata:
  name: h5p-staticfiles-server-svc
  namespace: {{ NAMESPACE }}
  labels:
    app: h5p-staticfiles-server
spec:
  type: ClusterIP
  ports:
    - port: {{ H5P_PORT }}
      targetPort: {{ H5P_PORT }}
      protocol: TCP
      name: h5p-staticfiles-server
  selector:
    app: h5p-staticfiles-server
